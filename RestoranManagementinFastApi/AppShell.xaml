<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="RestoranManagementinFastApi.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:RestoranManagementinFastApi"
    Title=""
    xmlns:pages="clr-namespace:RestoranManagementinFastApi.Pages"
    Shell.NavBarIsVisible="False">

    <ShellContent

        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent
    ContentTemplate="{DataTemplate pages:Login}"
    Route="RestaurantManagerLogin" />
    <ShellContent
    ContentTemplate="{DataTemplate pages:WaiterLogin}"
    Route="WaiterLogin" />
    <ShellContent
    ContentTemplate="{DataTemplate pages:RestaurantManagerPanel}"
    Route="RestaurantManagerPanel" />
    <ShellContent
    ContentTemplate="{DataTemplate pages:WaiterPanel}"
    Route="WaiterPanel" />


</Shell>
